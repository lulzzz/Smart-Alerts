      <ProjectItem ReplaceParameters="true" OpenInEditor="true">MyAlert.cs</ProjectItem>
      <ProjectItem ReplaceParameters="true" OpenInEditor="false">Manifest.json</ProjectItem>
    </Project>
  </TemplateContent>
</VSTemplate>